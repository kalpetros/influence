{
  "manifest_version": 2,
  "name": "Influence",
  "description": "Replace your new tab page",
  "version": "0.1",
  "permissions": [
    "management",
    "topSites",
    "bookmarks",
    "history",
    "downloads",
    "system.cpu",
    "system.memory",
    "system.storage",
    "geolocation"
  ],
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }
}