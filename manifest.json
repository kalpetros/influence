{
  "manifest_version": 2,
  "name": "Influence",
  "description": "Replace your new tab page with an elegant material design dashboard.",
  "version": "2",
  "author": "kalpetros",
  "icons": {
    "16": "src/img/icons/16.png",
    "32": "src/img/icons/32.png",
    "48": "src/img/icons/48.png",
    "128": "src/img/icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "management",
    "topSites",
    "bookmarks",
    "history",
    "downloads",
    "system.cpu",
    "system.memory",
    "system.storage",
    "geolocation",
    "management"
  ]
}
